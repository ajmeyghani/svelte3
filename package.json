{
  "name": "svelte3-starter",
  "version": "1.0.0",
  "description": "a starter kit for svelte 3 beta.",
  "main": "src/index.js",
  "scripts": {
    "prestart": "rm -rf dist",
    "prebuild": "rm -rf dist",
    "start": "webpack -w",
    "dev:server": "http-server dist -p 9000 -c-1",
    "build": "webpack --mode=production",
    "gzip": "gzip -k dist/*.css dist/*.js",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.html\" webpack.config.js --fix"
  },
  "keywords": [
    "svelte3",
    "webpack",
    "starter kit",
    "boilerplate"
  ],
  "author": "AJ Meyghani",
  "license": "MIT",
  "devDependencies": {
    "@tivac/eslint-plugin-svelte": "^1.2.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-html": "^5.0.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.1",
    "svelte-loader": "^2.13.3",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "svelte": "3.0.0-beta.21"
  }
}
